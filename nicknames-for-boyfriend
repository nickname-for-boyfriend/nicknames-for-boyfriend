    <script>
        // --- Global Variables and Constants ---
        const canvas = document.getElementById('wheelCanvas');
        const ctx = canvas.getContext('2d');
        const spinWheelButton = document.getElementById('spinWheelButton');
        const spinIcon = document.getElementById('spinIcon');
        const resultModal = document.getElementById('resultModal');
        const modalContent = document.getElementById('modalContent');
        const winnerText = document.getElementById('winnerText');
        const entryCountEl = document.getElementById('entryCount');
        const pointerEl = document.getElementById('pointer');
        const settingsPanel = document.getElementById('settingsPanel');
        const settingsToggle = document.getElementById('settingsToggle');
        const soundToggle = document.getElementById('soundToggle');
        const soundOnIcon = document.getElementById('soundOnIcon');
        const soundOffIcon = document.getElementById('soundOffIcon');
        const closeModalBtn = document.getElementById('closeModalBtn');
        const generateNewBtn = document.getElementById('generateNewBtn');
        const keepNameBtn = document.getElementById('keepNameBtn');
        const spinDurationSlider = document.getElementById('spinDuration');
        const spinDurationValue = document.getElementById('spinDurationValue');
        const colorPickersContainer = document.getElementById('colorPickers');
        const savedNamesList = document.getElementById('savedNamesList');

        // Initial setup
        let nicknames = [];
        let totalRotation = 0;
        let isSpinning = false;
        let lastWinner = null;
        let isSoundEnabled = true;
        let spinDuration = 5;
        let settings = { winSound: true };
        let savedNames = [];

        // Romantic color palette
        //let colors = ['#f8e3e7', '#e7bace', '#d9a5c5', '#c895b9', '#e3687e', '#d14a76'];
        let colors = ['#ffd1dc', '#b8d8e6', '#c6f7d0', '#ffe6a7', '#e6b8e6', '#ffccbc', '#87d4c5', '#ffb3de'];






        // Audio for celebration sound
        const celebrationSound = new Audio('sound.mp3');
        let isSoundPlaying = false;

        // Boyfriend Nickname Databases
        const nicknameDatabases = {
            romantic: [
                'My Love', 'My Heart', 'My Everything', 'My Soulmate', 'My Forever', 'My Destiny', 'My King', 'My Hero', 
                'My Prince', 'My Angel', 'My Sunshine', 'My World', 'My Dream', 'My Happiness', 'My Treasure', 'My Darling',
                'Sweetheart', 'Beloved', 'Dearest', 'Precious', 'Adored', 'Cherished', 'Darling', 'Honey', 'Sweetie'
            ],
            cute: [
                'Cutie Pie', 'Sweet Cheeks', 'Babe', 'Baby', 'Baby Boy', 'Boo', 'Boo Bear', 'Snuggle Bug', 'Cuddle Bear',
                'Sweet Pea', 'Sugar', 'Honey Bun', 'Pumpkin', 'Cupcake', 'Muffin', 'Cookie', 'Teddy Bear', 'Pookie',
                'Love Bug', 'Snookums', 'Buttercup', 'Sweet Stuff', 'Honey Bee', 'Sugar Plum', 'Peaches'
            ],
            flirty: [
                'Hot Stuff', 'Handsome', 'Stud', 'Hunk', 'Mr. Perfect', 'Dream Boat', 'Heartthrob', 'Sexy', 'Gorgeous',
                'Irresistible', 'Charming', 'Dashing', 'Captivating', 'Alluring', 'Tempting', 'Magnetic', 'Sizzling',
                'Smoking Hot', 'Fine', 'Delicious', 'Yummy', 'Tasty', 'Irresistible', 'Temptation', 'Forbidden Fruit'
            ],
            protective: [
                'My Protector', 'My Guardian', 'My Shield', 'My Rock', 'My Fortress', 'My Strength', 'My Anchor', 'My Safe Place',
                'My Hero', 'My Champion', 'My Knight', 'My Warrior', 'My Defender', 'My Shelter', 'My Harbor', 'My Sanctuary',
                'My Support', 'My Foundation', 'My Backbone', 'My Pillar', 'My Tower', 'My Mountain', 'My Oak', 'My Lion', 'My Bear'
            ],
            food: [
                'Honey', 'Sugar', 'Sweetie Pie', 'Cupcake', 'Muffin', 'Cookie', 'Brownie', 'Pudding', 'Jelly Bean', 'Gumdrop',
                'Lollipop', 'Marshmallow', 'Toffee', 'Fudge', 'Caramel', 'Chocolate', 'Cinnamon', 'Nutmeg', 'Spice', 'Pepper',
                'Apple Pie', 'Cherry', 'Peach', 'Berry', 'Melon', 'Kiwi', 'Mango', 'Papaya', 'Pineapple', 'Strawberry'
            ],
            animal: [
                'Bear', 'Lion', 'Tiger', 'Wolf', 'Fox', 'Eagle', 'Hawk', 'Falcon', 'Stallion', 'Bull',
                'Puppy', 'Kitty', 'Cub', 'Fawn', 'Lamb', 'Chick', 'Duckling', 'Birdie', 'Bunny', 'Panda',
                'Koala', 'Dolphin', 'Seal', 'Otter', 'Penguin', 'Owl', 'Parrot', 'Swan', 'Peacock', 'Phoenix'
            ],
            royal: [
                'My King', 'My Prince', 'My Lord', 'Your Highness', 'Your Majesty', 'My Royal', 'My Emperor', 'My Sovereign',
                'My Ruler', 'My Crown', 'My Throne', 'My Scepter', 'My Regal', 'My Noble', 'My Aristocrat', 'My Duke',
                'My Baron', 'My Knight', 'My Champion', 'My Paladin', 'My Guardian', 'My Protector', 'My Defender', 'My Hero'
            ]
        };

        // Initialize with generated nicknames and load saved names
        generateNicknames();
        loadSavedNames();

        // --- Nickname Generation Functions ---
        function generateNicknames() {
            const nicknameStyle = document.getElementById('nicknameStyle').value;
            const nameLength = document.getElementById('nameLength').value;
            const count = parseInt(document.getElementById('nameCount').value);
            
            nicknames = [];
            
            for (let i = 0; i < count; i++) {
                let nickname = '';
                
                // For random mix, pick a random style
                const style = nicknameStyle === 'random' ? 
                    Object.keys(nicknameDatabases)[Math.floor(Math.random() * Object.keys(nicknameDatabases).length)] : 
                    nicknameStyle;
                
                nickname = generateBoyfriendNickname(style, nameLength);
                nicknames.push(nickname);
            }
            
            updateWheel();
        }

        function generateBoyfriendNickname(style, length) {
            const nameList = nicknameDatabases[style];
            let name = nameList[Math.floor(Math.random() * nameList.length)];
            
            // For longer names, sometimes add "My" or other romantic prefixes
            if (length === 'long' && Math.random() < 0.7 && !name.startsWith('My ')) {
                const prefixes = ['My Beloved', 'My Dearest', 'My Precious', 'My Adored', 'My Cherished'];
                const prefix = prefixes[Math.floor(Math.random() * prefixes.length)];
                name = prefix + ' ' + name;
            }
            // For medium names, sometimes add romantic adjectives
            else if (length === 'medium' && Math.random() < 0.5) {
                const adjectives = ['Sweet', 'Dear', 'Precious', 'Adorable', 'Lovely', 'Wonderful', 'Amazing', 'Incredible'];
                const adjective = adjectives[Math.floor(Math.random() * adjectives.length)];
                
                if (!name.startsWith('My ')) {
                    name = adjective + ' ' + name;
                }
            }
            
            return name;
        }

        // --- Saved Names Functions ---
        function loadSavedNames() {
            const saved = localStorage.getItem('savedBoyfriendNames');
            if (saved) {
                savedNames = JSON.parse(saved);
                updateSavedNamesDisplay();
            }
        }

        function saveName(name) {
            if (!savedNames.includes(name)) {
                savedNames.push(name);
                localStorage.setItem('savedBoyfriendNames', JSON.stringify(savedNames));
                updateSavedNamesDisplay();
            }
        }

        function updateSavedNamesDisplay() {
            savedNamesList.innerHTML = '';
            
            if (savedNames.length === 0) {
                savedNamesList.innerHTML = '<p class="text-red-600 text-sm text-center py-2">No saved nicknames yet</p>';
                return;
            }
            
            savedNames.forEach((name, index) => {
                const nameItem = document.createElement('div');
                nameItem.className = 'saved-name-item flex justify-between items-center p-2 rounded-lg romantic-font';
                nameItem.innerHTML = `
                    <span class="text-red-800 font-medium">${name}</span>
                    <button onclick="removeSavedName(${index})" class="text-red-600 hover:text-red-800 transition duration-200">
                        <svg xmlns="http://www.w3.org/2000/svg" class="h-4 w-4" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12" />
                        </svg>
                    </button>
                `;
                savedNamesList.appendChild(nameItem);
            });
        }

        function removeSavedName(index) {
            savedNames.splice(index, 1);
            localStorage.setItem('savedBoyfriendNames', JSON.stringify(savedNames));
            updateSavedNamesDisplay();
        }

        function clearSavedNames() {
            if (confirm('Are you sure you want to clear all saved nicknames?')) {
                savedNames = [];
                localStorage.removeItem('savedBoyfriendNames');
                updateSavedNamesDisplay();
            }
        }

        function exportSavedNames() {
            if (savedNames.length === 0) {
                alert('No saved nicknames to export.');
                return;
            }
            
            const blob = new Blob([savedNames.join('\n')], { type: 'text/plain' });
            const url = URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = 'boyfriend-nicknames.txt';
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            URL.revokeObjectURL(url);
        }

        // --- Utility Functions ---
        function updateWheel() {
            const total = nicknames.length;

            if (total === 0) {
                spinWheelButton.disabled = true;
                colors = ['#fecaca','#fca5a5','#f87171','#ef4444','#dc2626','#b91c1c','#fdba74','#f59e0b'];
            } else {
                spinWheelButton.disabled = isSpinning;
                while (colors.length < nicknames.length) {
                    colors.push(colors[Math.floor(Math.random() * colors.length)]);
                }
                colors = colors.slice(0, nicknames.length);
            }

            entryCountEl.textContent = `${nicknames.length} generated nicknames.`;
            updateColorPickers();
            drawWheel();
        }

        function updateColorPickers() {
            colorPickersContainer.innerHTML = '';
            nicknames.forEach((_, index) => {
                const picker = document.createElement('input');
                picker.type = 'color';
                picker.className = 'color-picker';
                picker.value = colors[index] || '#fecaca';
                picker.addEventListener('change', (e) => {
                    colors[index] = e.target.value;
                    drawWheel();
                });
                colorPickersContainer.appendChild(picker);
            });
        }

        // --- Canvas Drawing Logic ---
        function setCanvasSize() {
            const container = document.getElementById('canvas-container');
            const size = container.offsetWidth;
            canvas.width = size;
            canvas.height = size;
        }

        window.addEventListener('resize', () => {
            setCanvasSize();
            drawWheel();
        });

        // Initial size setup
        setCanvasSize();

        function drawWheel() {
            const centerX = canvas.width / 2;
            const centerY = canvas.height / 2;
            const radius = Math.min(centerX, centerY) * 0.98;
            const total = nicknames.length;

            if (total === 0) {
                ctx.clearRect(0, 0, canvas.width, canvas.height);
                ctx.textAlign = 'center';
                ctx.fillStyle = '#dc2626';
                ctx.font = '20px Dancing Script';
                ctx.fillText('Generate nicknames first!', centerX, centerY);
                return;
            }

            const arc = (2 * Math.PI) / total;
            ctx.clearRect(0, 0, canvas.width, canvas.height);

            // Apply current rotation
            ctx.save();
            ctx.translate(centerX, centerY);
            ctx.rotate(totalRotation * (Math.PI / 180));

            // Dynamic Font Size
            const fontSize = total > 15 ? 12 : (total > 8 ? 14 : 16);
            ctx.font = `600 ${fontSize}px Dancing Script`;

            for (let i = 0; i < total; i++) {
                const startAngle = i * arc;
                const endAngle = (i + 1) * arc;

                // Draw slice
                ctx.beginPath();
                ctx.moveTo(0, 0);
                ctx.arc(0, 0, radius, startAngle, endAngle);
                ctx.closePath();
                ctx.fillStyle = colors[i % colors.length];
                ctx.fill();

                // Outline
                ctx.strokeStyle = '#fff';
                ctx.lineWidth = 3;
                ctx.stroke();

                // Draw Text
                ctx.save();
                ctx.rotate(startAngle + arc / 2);
                ctx.textAlign = 'right';
                ctx.fillStyle = '#7c2d12';
                const textX = radius * 0.85;

                let nameToDisplay = nicknames[i];
                if (total > 8 && nameToDisplay.length > 15) {
                    nameToDisplay = nameToDisplay.substring(0, 15) + '...';
                }

                ctx.fillText(nameToDisplay, textX, 5);
                ctx.restore();
            }

            ctx.restore();

            // Center hub
            ctx.beginPath();
            ctx.arc(centerX, centerY, radius * 0.02, 0, 2 * Math.PI);
            ctx.fillStyle = '#fff';
            ctx.fill();
            ctx.strokeStyle = '#dc2626';
            ctx.lineWidth = 4;
            ctx.stroke();
            
            // Update pointer color to match the current top segment
            updatePointerColor();
        }

        function updatePointerColor() {
            const total = nicknames.length;
            if (total === 0) {
                pointerEl.style.borderColor = '#dc2626 transparent transparent transparent';
                return;
            }
            
            const winningIndex = calculateWinnerIndex();
            const segmentColor = colors[winningIndex];
            pointerEl.style.borderColor = `${segmentColor} transparent transparent transparent`;
        }

        // --- Spinning Logic ---
        function startSpin() {
            if (isSpinning || nicknames.length === 0) return;
            isSpinning = true;
            spinWheelButton.disabled = true;
            spinIcon.classList.add('spinning');

            const extraRotations = Math.floor(Math.random() * 6 + 7);
            const randomAngle = Math.random() * 360;
            const finalTarget = totalRotation + extraRotations * 360 + randomAngle;

            animateSpin(finalTarget);
        }

        function animateSpin(finalTarget) {
            let startTime = null;
            const duration = spinDuration * 1000;
            const startRotation = totalRotation;

            function easeOutQuint(t) { return 1 - Math.pow(1 - t, 5); }

            function step(timestamp) {
                if (!startTime) startTime = timestamp;
                const elapsed = timestamp - startTime;
                let t = Math.min(1, elapsed / duration);
                let easedT = easeOutQuint(t);
                const newRotation = startRotation + (finalTarget - startRotation) * easedT;

                totalRotation = newRotation % 360;
                drawWheel();

                if (t < 1) {
                    requestAnimationFrame(step);
                } else {
                    totalRotation = finalTarget % 360;
                    drawWheel();
                    spinIcon.classList.remove('spinning');
                    const winningIndex = calculateWinnerIndex();
                    showWinner(winningIndex);
                    isSpinning = false;
                    spinWheelButton.disabled = nicknames.length === 0;
                }
            }

            requestAnimationFrame(step);
        }

        function calculateWinnerIndex() {
            const total = nicknames.length;
            if (total === 0) return 0;

            const sliceAngle = 360 / total;
            const pointerDeg = 270;

            for (let i = 0; i < total; i++) {
                let start = (i * sliceAngle + totalRotation) % 360;
                if (start < 0) start += 360;
                let end = (start + sliceAngle) % 360;

                if (start < end) {
                    if (pointerDeg >= start && pointerDeg < end) {
                        return i;
                    }
                } else {
                    if (pointerDeg >= start || pointerDeg < end) {
                        return i;
                    }
                }
            }

            return 0;
        }

        function showWinner(index) {
            lastWinner = nicknames[index];
            winnerText.textContent = lastWinner;

            resultModal.classList.remove('hidden');
            modalContent.classList.add('modal-animated');

            createFallingHearts();

            if (isSoundEnabled && settings.winSound) {
                celebrationSound.currentTime = 0;
                celebrationSound.play().then(() => {
                    isSoundPlaying = true;
                }).catch(e => console.log('Audio play failed:', e));
            }

            createConfetti();
        }

        function createFallingHearts() {
            const hearts = ['‚ù§Ô∏è', 'üíï', 'üíñ', 'üíó', 'üíì', 'üíû', 'üíò', 'üíù', 'üß°', 'üíõ', 'üíö', 'üíô', 'üíú'];
            
            for (let i = 0; i < 40; i++) {
                const heartElement = document.createElement('div');
                heartElement.className = 'floating-heart';
                heartElement.textContent = hearts[Math.floor(Math.random() * hearts.length)];
                heartElement.style.left = Math.random() * 100 + 'vw';
                heartElement.style.animationDelay = Math.random() * 2 + 's';
                document.body.appendChild(heartElement);

                setTimeout(() => {
                    if (heartElement.parentNode) {
                        document.body.removeChild(heartElement);
                    }
                }, 8000);
            }
        }

        function createConfetti() {
            const confettiContainer = document.createElement('div');
            confettiContainer.id = 'confetti-container';
            confettiContainer.style.position = 'fixed';
            confettiContainer.style.top = '0';
            confettiContainer.style.left = '0';
            confettiContainer.style.width = '100%';
            confettiContainer.style.height = '100%';
            confettiContainer.style.pointerEvents = 'none';
            confettiContainer.style.zIndex = '40';
            document.body.appendChild(confettiContainer);

            const colors = ['#fecaca', '#fca5a5', '#f87171', '#ef4444', '#dc2626', '#b91c1c', '#fdba74', '#f59e0b'];

            for (let i = 0; i < 200; i++) {
                const confetti = document.createElement('div');
                confetti.className = 'confetti';
                confetti.style.left = Math.random() * 100 + 'vw';
                confetti.style.animationDelay = Math.random() * 3 + 's';
                confetti.style.animationDuration = (Math.random() * 2 + 2) + 's';
                confetti.style.backgroundColor = colors[Math.floor(Math.random() * colors.length)];
                confetti.style.animation = 'confetti-fall linear forwards';
                confettiContainer.appendChild(confetti);
            }

            setTimeout(() => {
                if (document.getElementById('confetti-container')) {
                    document.body.removeChild(confettiContainer);
                }
            }, 6000);
        }

        function generateNewAndContinue() {
            generateNicknames();
            closeModal();
        }

        function keepNameAndContinue() {
            if (lastWinner) {
                saveName(lastWinner);
            }
            closeModal();
        }

        function closeModal() {
            if (isSoundPlaying) {
                celebrationSound.pause();
                celebrationSound.currentTime = 0;
                isSoundPlaying = false;
            }

            modalContent.classList.remove('modal-animated');
            setTimeout(() => {
                resultModal.classList.add('hidden');
                lastWinner = null;
            }, 50);
        }

        function toggleSound() {
            isSoundEnabled = !isSoundEnabled;
            if (isSoundEnabled) {
                soundOnIcon.classList.remove('hidden');
                soundOffIcon.classList.add('hidden');
            } else {
                soundOnIcon.classList.add('hidden');
                soundOffIcon.classList.remove('hidden');
            }
        }

        function toggleSettings() {
            if (settingsPanel.classList.contains('hidden')) {
                settingsPanel.classList.remove('hidden');
            } else {
                settingsPanel.classList.add('hidden');
            }
        }

        function updateSpinDuration() {
            spinDuration = parseInt(spinDurationSlider.value);
            spinDurationValue.textContent = `${spinDuration} seconds`;
        }

        // --- Initialization ---
        window.onload = function() {
            soundToggle.addEventListener('click', toggleSound);
            settingsToggle.addEventListener('click', toggleSettings);
            closeModalBtn.addEventListener('click', closeModal);
            generateNewBtn.addEventListener('click', generateNewAndContinue);
            keepNameBtn.addEventListener('click', keepNameAndContinue);
            spinDurationSlider.addEventListener('input', updateSpinDuration);

            const savedSettings = localStorage.getItem('boyfriendWheelSettings');
            if (savedSettings) {
                settings = JSON.parse(savedSettings);
                document.getElementById('winSoundToggle').checked = settings.winSound;
            }

            document.getElementById('winSoundToggle').addEventListener('change', function() {
                settings.winSound = this.checked;
                localStorage.setItem('boyfriendWheelSettings', JSON.stringify(settings));
            });

            updateSpinDuration();
            updateWheel();
        }

        // Expose functions globally for HTML elements
        window.startSpin = startSpin;
        window.closeModal = closeModal;
        window.updateWheel = updateWheel;
        window.generateNicknames = generateNicknames;
        window.generateNewAndContinue = generateNewAndContinue;
        window.keepNameAndContinue = keepNameAndContinue;
        window.removeSavedName = removeSavedName;
        window.clearSavedNames = clearSavedNames;
        window.exportSavedNames = exportSavedNames;

    </script>
</body>
</html>
